    "fs-extra": "^11.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "web3-contract": "https://github.com/gatechain/web3-contract#3.1.0",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  },
  "peerDependencies": {
    "web3-contract": "https://github.com/gatechain/web3-contract#3.1.0"
  }
}
